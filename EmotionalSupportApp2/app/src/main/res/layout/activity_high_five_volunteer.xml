<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".HighFiveVolunteerActivity">


    <fragment
        android:id="@+id/google_map"
        android:name="com.google.android.gms.maps.SupportMapFragment"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".MapsActivity" />

    <Button
        android:id="@+id/confirm_high_five"
        android:layout_width="wrap_content"
        android:layout_height="75dp"
        android:layout_alignBottom="@id/google_map"
        android:layout_marginLeft="250dp"
        android:onClick="confirmRequest"
        android:text="Confirm High Five" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="75dp"
        android:id="@+id/deny_high_five"
        android:text="Deny High Five"
        android:layout_alignBottom="@id/google_map"
        android:onClick="returnHome"
        />

</RelativeLayout>